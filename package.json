		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^8.0.0",
		"start-server-webpack-plugin": "^2.2.5",
		"style-loader": "^1.0.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-middleware": "^3.7.2",
		"webpack-hot-middleware": "^2.25.0",
		"webpack-node-externals": "^1.7.2"
	}
}
