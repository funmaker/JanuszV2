{
	"name": "janusz-v2",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.0.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"scripts": {
		"prestart": "npm run lint || true",
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r web/static dist/ && cp -r sounds/sounds dist/",
		"build:prod-package": "babel-node webpack/package.babel.js",
		"lint": "eslint --ext .js --ext .jsx --ignore-path .gitignore --rulesdir webpack/linter .",
		"lint-fix": "eslint --ext .js --ext .jsx --ignore-path .gitignore --rulesdir webpack/linter . --fix"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"ansi-to-html": "^0.6.13",
		"axios": "^0.21.1",
		"babel-eslint": "^10.0.3",
		"bezier-easing": "^2.1.0",
		"body-parser": "^1.19.0",
		"chalk": "^3.0.0",
		"chokidar": "^3.3.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.4",
		"detect-node": "^2.0.4",
		"discord.js": "^11.5.1",
		"express": "^4.17.1",
		"express-promise-router": "^3.0.3",
		"express-ws": "^4.0.0",
		"fluent-ffmpeg": "^2.1.2",
		"franc-min": "^4.1.1",
		"fs-extra": "^8.1.0",
		"http-status-codes": "^1.4.0",
		"intercept-stdout": "^0.1.2",
		"iso-639-3": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mobile-device-detect": "^0.3.3",
		"morgan": "^1.9.1",
		"mumble": "github:funmaker/node12-mumble",
		"ndfft": "0.0.0",
		"node-opus": "^0.3.3",
		"prop-types": "^15.7.2",
		"query-string": "^6.9.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-hot-loader": "^4.12.18",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-split-pane": "^0.1.89",
		"semantic-ui-react": "^0.88.1",
		"source-map-support": "^0.5.16",
		"strip-ansi": "^6.0.0",
		"uuid": "^3.3.3",
		"web-audio-engine": "^0.13.4",
		"youtube-dl": "^2.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.2",
		"@babel/node": "^7.7.0",
		"@babel/plugin-proposal-decorators": "^7.7.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/polyfill": "^7.7.0",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-react": "^7.7.0",
		"@babel/register": "^7.7.0",
		"@hot-loader/react-dom": "^16.11.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.2.0",
		"eslint": "^6.6.0",
		"eslint-plugin-import": "^2.18.2",
